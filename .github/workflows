name: auto_struct

on: [push]

jobs:
  build:
    runs-on: ubuntu-latest
    strategy:
      matrix:
        python-version: ["3.11"]
    steps:
      -uses: actions/checkout@v3
      -name: Set up Python ${{ matrix.python-version }}
       uses: action/setup-python@v4
       with:
         python-version: ${{ matrix.python-version }}
      -name: Test with pytest
       run: |
         pytest tests/
